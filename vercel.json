{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/api/users", "dest": "/api/router.js" },
    { "src": "/api/other", "dest": "/api/router2.js" },
    { "src": "/api/another", "dest": "/api/anotherRouter.js" }
  ],
  "env": {
    "CONFIG": "$CONFIG"
  },
  "envSecrets": {
    "CONFIG": {
      "type": "json"
    }
  }
}
