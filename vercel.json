{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "env": {
          "NODE_ENV": "production"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/api/login2",
      "dest": "./router/router2.js"
    },
    {
      "src": "/api/protected",
      "dest": "./router/router2.js"
    },
    {
      "src": "/user-data",
      "dest": "./router/route.js"
    },
    {
      "src": "/register",
      "dest": "./router/route.js"
    },
    {
      "src": "/resetPassword",
      "dest": "./router/route.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "http://localhost:8080/api/$1",
      "headers": {
        "host": "localhost:8080"
      },
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "ATLAS_URI": "$ATLAS_URI",
    "JWT_SECRET": "$JWT_SECRET",
    "EMAIL": "$EMAIL",
    "PASSWORD": "$PASSWORD"
  }
}
