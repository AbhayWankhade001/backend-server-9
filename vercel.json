{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/user-data", "dest": "/router/route.js" },
    { "src": "/register", "dest": "/router/route.js" },
    { "src": "/addData", "dest": "/router/route.js" },
    { "src": "/login", "dest": "/router/router2.js" }
  ],
  {
  "src": "/api/(.*)",
  "dest": "http://localhost:8080/api/$1",
  "headers": {
    "host": "localhost:8080"
  },
  "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
},
  "env": {
    "CONFIG": "$CONFIG"
  }
}
