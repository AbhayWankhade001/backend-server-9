{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/user-data", "dest": "/router/route.js" },
    { "src": "/register", "dest": "/router/route.js" },
    { "src": "/addData", "dest": "/router/route.js" },
     { "src": "/login", "dest": "/router/router2.js" },
  ],
  "env": {
    "CONFIG": "$CONFIG"
  }
}
