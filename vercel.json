{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "router2.js",
      "use": "@vercel/node"
    },
    {
      "src": "route.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/login2",
      "dest": "./router2.js"
    },
    {
      "src": "/api/protected",
      "dest": "./router2.js"
    },
    {
      "src": "/user-data",
      "dest": "./route.js"
    },
    {
      "src": "/register",
      "dest": "./route.js"
    },
    {
      "src": "/resetPassword",
      "dest": "./route.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "http://localhost:8080/api/$1",
      "headers": {
        "host": "localhost:8080"
      },
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/router/(.*)",
      "dest": "./$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "./index.js"
    }
  ]
}
