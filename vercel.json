{
    "version": 2,
    "builds": [{ "src": "index.js", "use": "@now/node-server" }],
    "routes": [
      {
        "src": "/login2",
        "dest": "/router/router2.js",
        "methods": ["GET"]
      },
      {
        "src": "/protected",
        "dest": "/router/router2.js",
        "methods": ["GET"]
      },
      {
        "src": "/register",
        "dest": "/router/route.js",
        "methods": ["POST"]
      },
      {
        "src": "/user-data",
        "dest": "/router/route.js",
        "methods": ["GET"]
      },
      {
        "src": "/resetPassword",
        "dest": "/router/route.js",
        "methods": ["POST"]
      },
      {
        "src": "/addData",
        "dest": "/router/route.js",
        "methods": ["POST"]
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  
